#include <stdio.h>

int main()
{
    int a[10], sum,i,j,count=0;
    for (i=0;i<10;i++)
    scanf("%d",&a[i]);
    printf("Enter the Sum: ");
    scanf("%d",&sum);
    for(i=0;i<10;i++)
        for (j=i+1;j<10;j++)
            if (a[i]+a[j]==sum)
                count++;
    printf("Number of Pairs: %d", count);
    

    return 0;
}
